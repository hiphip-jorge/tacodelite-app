# Taco Delite App Environment Variables
# Copy this file to .env.local and update with your actual values

# API Gateway URL (update after Terraform deployment)
VITE_API_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod

# Development Mock Mode (set to 'true' to use mock data instead of API calls)
VITE_USE_MOCK=true

# App Configuration
VITE_APP_NAME=Taco Delite
VITE_ENVIRONMENT=staging
