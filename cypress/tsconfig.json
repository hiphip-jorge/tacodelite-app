{
  "exclude": [
    "../node_modules"
  ],
  "include": [
    "e2e/**/*",
    "support/**/*",
    "../node_modules/cypress"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "types": ["node"],
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "target": "es2019",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "lib": ["es2019", "dom"],
    "typeRoots": ["../node_modules/@types"]
  }
}
